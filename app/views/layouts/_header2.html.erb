<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "WHATIMEISIT", root_path, id: "logo" %>
    <nav>
      <ul class="nav navbar-nav pull-right">
        <li><%= link_to "Home", root_path %></li>
     
        <% if user_logged_in %>
          <li><%= link_to "Users", user_path(current_user.id) %></li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Account git<b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to " Edit Profile", edit_user_path(current_user.id) %></li>
              <li><%= link_to "Settings", '#' %></li>
              <li class="divider"></li>
              <li>
            <%= link_to 'Sign Out', sign_out_path(current_user.id), method: :delete %>
              </li>
            </ul>
          </li>
          
        <%else%>
          <li><%= link_to "Sign in", sign_in_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>